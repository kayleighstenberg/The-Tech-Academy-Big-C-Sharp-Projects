@using NewsLetterAppMVC.ViewModels
@model List<SignUpVm>

<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Quotes</h5>
        </div>
        <div class="card-body">
            @foreach (var signup in Model)
            {

                    <input type="hidden" name="Id" value="@signup.Id" />
                    <div class="row mb-3">
                        <div class="col-2">
                            @signup.FirstName
                        </div>
                        <div class="col-2">
                            @signup.LastName
                        </div>
                        <div class="col-4">
                            @signup.EmailAddress
                        </div>
                        <div clss="col-2">
                            @signup.QuoteAmt
                        </div>
                    </div>

            }

        </div>
    </div>
</div>

<form method="get" action="/Home">
    <button type="submit">Quote Page</button>
</form>